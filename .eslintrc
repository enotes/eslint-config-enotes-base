{
  "env": {
    "es6": true,
    "commonjs": true,
    "node": true,
    "browser": true,
    "mocha": true
  },
  "globals": {},
  "rules": {
    "array-bracket-spacing": [
      2,
      "never"
    ],
    "block-scoped-var": 2,
    "brace-style": [
      2,
      "1tbs"
    ],
    "camelcase": 2,
    "computed-property-spacing": [
      2,
      "never"
    ],
    "comma-style": [
      2,
      "last"
    ],
    "curly": 2,
    "dot-notation": [
      2,
      {
        "allowKeywords": true
      }
    ],
    "eol-last": 2,
    "eqeqeq": [
      2,
      "smart"
    ],
    "guard-for-in": 2,
    "indent": [
      2,
      4,
      {
        "SwitchCase": 1
      }
    ],
    "no-bitwise": 2,
    "new-cap": 1,
    "no-caller": 2,
    "no-extra-parens": 0,
    "no-empty": 2,
    "no-eval": 2,
    "no-extend-native": 2,
    "no-irregular-whitespace": 2,
    "no-loop-func": 2,
    "no-mixed-spaces-and-tabs": 2,
    "no-new": 2,
    "no-plusplus": 2,
    "no-trailing-spaces": 2,
    "no-undef": 2,
    "no-unused-vars": 2,
    "no-use-before-define": [
      2,
      {
        "functions": false
      }
    ],
    "no-shadow": 2,
    "object-curly-spacing": [
      2,
      "never"
    ],
    "quotes": [
      2,
      "single"
    ],
    "space-unary-ops": [
      2,
      {
        "words": true,
        "nonwords": false
      }
    ],
    "keyword-spacing": [
      2,
      {
        "before": true,
        "after": true
      }
    ],
    "wrap-iife": [
      2,
      "any"
    ],
    "no-const-assign": "error"
  }
}
